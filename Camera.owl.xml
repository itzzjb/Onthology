<!-- Main use cases is to is to define Classes, their Relationships and their Properties. -->
<!-- Compared to RDFS expresses much more complex and richer relaionships -->

<!-- 1. Express relationships amongs classes defined in different documents accross web -->
<!-- 2. Construct new classes by unions, intersections, and complements of existing classes -->
<!-- 3. Add constraints on the number and type of properties of classes -->
<!-- 4. Determine if all members of a class will have a certain property or only some of them might -->

<!-- Defining a Class in OWL -->
<!-- A class defined in a OWL is a subclass of owl:Thing. owl:Thing is the root class of all classes. -->
<!-- owl:Thing > rdfs:Resource > rdfs:Class > owl:Class -->
<!-- owl:Class is a subclass of rdfs:Class -->
<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">
    
    <owl:Class rdf:ID="Camera">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:Class>
</rdf:RDF>



<!-- Convert the Class definitions on Camera.rdfs to OWL -->
<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">  

    <owl:Class rdf:ID="Camera">
    </owl:Class>

    <owl:Class rdf:ID="Person">
    </owl:Class>

    <owl:Class rdf:ID="Specifications">
    </owl:Class>

    <owl:Class rdf:ID="Digital"> 
        <rdfs:subClassOf rdf:resource="#Camera"/>
    </owl:Class>

    <owl:Class rdf:ID="Film"> 
        <rdfs:subClassOf rdf:resource="#Camera"/>
    </owl:Class>
    
    <owl:Class rdf:ID="SLR"> 
        <rdfs:subClassOf rdf:resource="#Digital"/>
    </owl:Class>   
    
    <!-- Point-And-Shoot class form RDFS is changes to PointAndShoot because some validators do not accept hyphens in class names -->
    <owl:Class rdf:ID="PointAndShoot"> 
        <rdfs:subClassOf rdf:resource="#Digital"/>
    </owl:Class>
    <owl:Class rdf:ID="Photographer"> 
        <rdfs:subClassOf rdf:resource="#Person"/>
    </owl:Class>

</rdf:RDF>



<!-- Owl Class Features - owl:allValuesFrom -->
<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">  

    <!-- #### Classes #### -->
    
    <owl:Class rdf:ID="SLR"> 
        <rdfs:subClassOf rdf:resource="#Digital"/>
    </owl:Class>

    <owl:Class rdf:ID="Photographer"> 
        <rdfs:subClassOf rdf:resource="#Person"/>
    </owl:Class>


    <!-- Following is the owned_by property -->
    <!-- Because subclasses inherit properties from their superclasses -->
    <!-- This means that ExpensiveSLR can be owned_by both Professionals and Amateurs -->
    <rdfs:Property rdf:ID="owned_by">
        <rdfs:domain rdf:resource="#Camera"/>
        <rdfs:range rdf:resource="#Photographer"/>
    </rdfs:Property>

    <!-- Professional and Amateur are subclasses of SLR -->
    <owl:Class rdf:ID="Professional"> 
        <rdfs:subClassOf rdf:resource="#Photographer"/>
    </owl:Class>

    <owl:Class rdf:ID="Amateur"> 
        <rdfs:subClassOf rdf:resource="#Photographer"/>
    </owl:Class>
    
    <!-- Expensive SLR is a subclass of SLR -->
    <!-- Inorder to explicitly express that ExpensiveSLR can only be owned_by Professionals -->
    <owl:Class rdf:ID="ExpensiveSLR">
        <rdfs:subClassOf rdf:resource="#SLR"/>
        <rdfs:subClassOf>
            <!-- owl:Restriction is used to specify a anonymous class -->
            <owl:Restriction>
                <!-- owl:onProperty specifies the property to which the restriction applies -->
                <owl:onProperty rdf:resource="#owned_by"/>
                <!-- owl:allValuesFrom specifies that all values of the property must be from the specified class -->
                <owl:allValuesFrom rdf:resource="#Professional"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

</rdf:RDF>



<!-- Owl Class Features - owl:someValuesFrom -->
<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">  

    <!-- #### Classes #### -->
    
    <owl:Class rdf:ID="SLR"> 
        <rdfs:subClassOf rdf:resource="#Digital"/>
    </owl:Class>

    <owl:Class rdf:ID="Photographer"> 
        <rdfs:subClassOf rdf:resource="#Person"/>
    </owl:Class>

    <rdfs:Property rdf:ID="owned_by">
        <rdfs:domain rdf:resource="#Camera"/>
        <rdfs:range rdf:resource="#Photographer"/>
    </rdfs:Property>

    <owl:Class rdf:ID="Professional"> 
        <rdfs:subClassOf rdf:resource="#Photographer"/>
    </owl:Class>

    <owl:Class rdf:ID="Amateur"> 
        <rdfs:subClassOf rdf:resource="#Photographer"/>
    </owl:Class>
    
    <!-- Inorder loose some restrictions from before and give Amateurs the ability to own ExpensiveSLR 
    given the fact that atleast one of the owners is a Professional -->
    <owl:Class rdf:ID="ExpensiveSLR">
        <rdfs:subClassOf rdf:resource="#SLR"/>
        <rdfs:subClassOf>
            <!-- owl:Restriction is used to specify a anonymous class -->
            <owl:Restriction>
                <!-- owl:onProperty specifies the property to which the restriction applies -->
                <owl:onProperty rdf:resource="#owned_by"/>
                <!-- owl:someValuesFrom specifies that some values of the property must be from the specified class -->
                <owl:someValuesFrom rdf:resource="#Professional"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

</rdf:RDF>


<!-- Owl Class Features - owl:hasValue -->
<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">  

    <!-- #### Classes ### -->
    
    <owl:Class rdf:ID="SLR"> 
        <rdfs:subClassOf rdf:resource="#Digital"/>
    </owl:Class>

    <!-- A property called expensiveOrNot that returns a string -->
    <!-- owl:DatatypeProperty is used to define a property that links an individual to a data value -->
    <!-- You can assign expensive or inexpensive as values -->
    <!-- This property can be used by class Digital and subclasses SLR, PointAndShoot and ExpensiveSLR -->
    <owl:DatatypeProperty rdf:ID="expensiveOrNot">
        <rdfs:domain rdf:resource="#Digital"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    <!-- rdfs:dataType is used here to specify the datatype of the property values -->
    <rdfs:dataType rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
    
    <!-- We can constrain expeniveOrNot property to always be `expensive` for ExpensiveSLR class -->
    <owl:Class rdf:ID="ExpensiveSLR">
        <rdfs:subClassOf rdf:resource="#SLR"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#expensiveOrNot"/>
                <!-- owl:hasValue specifies that the property must have the specified value -->
                <!-- Notice that rdf:dataType is used to specify the datatype of the value -->
                <owl:hasValue rdf:dataType="http://www.w3.org/2001/XMLSchema#string">
                    expensive
                </owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

</rdf:RDF>



<!-- Owl Class Features - owl:cardinality, owl:minCardinality, owl:maxCardinality -->
<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">  

    <!-- #### Classes #### -->
    
    <owl:Class rdf:ID="SLR"> 
        <rdfs:subClassOf rdf:resource="#Digital"/>
    </owl:Class>

    <rdfs:Property rdf:ID="owned_by">
        <rdfs:domain rdf:resource="#Camera"/>
        <rdfs:range rdf:resource="#Photographer"/>
    </rdfs:Property>
    
    <!-- Here, ExpensiveSLR be able to have exactly one owner -->
    <owl:Class rdf:ID="ExpensiveSLR">
        <rdfs:subClassOf rdf:resource="#SLR"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#owned_by"/>
                <!-- owl:cardinality specifies that the property must have exactly the specified number of values -->
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">
                    1
                </owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>    

    <!-- Here, ExpensiveSLR need to have at least one owner -->
    <owl:Class rdf:ID="ExpensiveSLR">
        <rdfs:subClassOf rdf:resource="#SLR"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#owned_by"/>
                <!-- owl:minCardinality specifies that the property must have at least the specified number of values -->
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">
                    1
                </owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- Here, ExpensiveSLR can have at most three owners on maximum -->
    <owl:Class rdf:ID="ExpensiveSLR">
        <rdfs:subClassOf rdf:resource="#SLR"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#owned_by"/>
                <!-- owl:maxCardinality specifies that the property must have at most the specified number of values -->
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">
                    3
                </owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- Here, ExpensiveSLR can have at least one user but not more than three -->
    <owl:Class rdf:ID="ExpensiveSLR">
        <rdfs:subClassOf rdf:resource="#SLR"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#owned_by"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#owned_by"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>


</rdf:RDF>

<!-- OWL Class Features - Enumerations in OWL -->
<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:base="http://www.yuchen.net/photography/Camera.owl">

    <!-- #### Classes #### -->

    <!-- We can explicilty define what recources are qualified as ExpensiveSLRs -->
    <owl:Class rdf:ID="ExpensiveSLR">
        <rdfs:subClassOf rdf:resource="#SLR"/>
        <!-- owl:oneOf specifies an enumeration of instances -->
        <!-- Note hat rdf:parseType="Collection" is ussed with owl:oneOf -->
        <owl:oneOf rdf:parseType="Collection">
            <SLR rdf:about="http://www.someNikonSite.com#D70"/>
            <SLR rdf:about="http://www.someNikonSite.com#D200"/>
            <SLR rdf:about="http://www.someCanonSite.com#20D"/>
        </owl:oneOf>
    </owl:Class>


</rdf:RDF>

